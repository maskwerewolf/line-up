.header1{position: relative;margin: 0;background: url(<%= asset_path '80de_red.png'%>) center top no-repeat;height: 306px; }


.header .header-top{width: 950px;margin: auto;position: relative}
.header .header-top .bg1{background: url(<%= asset_path 'h-body.png'%>)no-repeat;height: 500px;padding-top: -300px;}

.header .header-bottom{background: url(<%= asset_path 'h-bottom.png' %>) repeat-x;position: relative;height: 233px;margin-top: -200px;}

.container-layout{background: #D42028 ;position: relative;margin: 0;padding: 0;height: auto;}
.container{position: relative;margin: 0 auto;width: 950px;height: auto;}

/*00A0E8 D42028*/
.h-ui1{position: relative;width: 948px;margin-top: -20px;border: 1px solid #DE0305;border-top-left-radius: 5px;border-top-right-radius: 5px;}
.h-ui1 .h-title{background: #DE0305 ;color: #fff;line-height: 30px;}
.h-ui1 .h-title strong{font-size: 14px;padding-left: 5px;}
.h-ui1 .h-body{position:relative;background: #FFFFFF;}

.h-ui1 .h-body .h-body-form{position:relative;padding-top: 10px;padding-left: 10px;height: 45px;}
input,button,select,textarea{outline:none}
.h-ui1 .h-body .h-body-form input,.h-ui1 .h-body .h-body-form img,.h-ui1 .h-body .h-body-form button{vertical-align:middle;}
.h-ui1 .h-body .h-body-form .inp1{border: 1px solid #DE0305;;width: 200px;height: 30px;padding-left:15px;line-height: 30px;}


.h-ui1 .h-body .h-body-form .inp2{border: 1px solid #DE0305;;width: 50px;height: 30px;margin-left:10px;}
.h-ui1 .h-body .h-body-form  span{margin-left: 10px;}
.h-ui1 .h-body .h-body-form .inp3{border: 1px solid #DE0305;;width: 50px;height: 30px;margin-left:5px;padding-left: 15px; line-height: 30px;}
.h-ui1 .h-body .h-body-form .btn1{background: #DE0305;height: 32px;border:0;width: 150px;text-align:center;color: #fff;font-weight: bold}

.h-ui1 .h-body .h-table{position: relative;border-top: 1px solid #DE0305;}
.h-ui1 .h-body .h-table ul{padding-left: 10px;margin: 0;list-style: none;}
/*.h-ui1 .h-body .h-table ul li{float: left;width: 185px;}*/
