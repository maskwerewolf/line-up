.c-left{position: relative;width: 640px; float: left;border: 1px solid #DE0305;border-top-left-radius: 5px;border-top-right-radius:5px;}
.c-left .b-ui3{position: relative;width: 640px;}
.c-left .b-ui3 h2{color: #fff;background:#DE0305;padding-left: 5px;}
.c-left .b-ui3 .tab{padding-left: 5px;}
.c-left .b-ui3 .tab p{line-height: 14px;}


.c-left .b-ui1{position: relative ;width: 640px; height: 713px; }
.c-left .b-ui1 table th{border-bottom: 1px solid #de0305 ;border-top: 1px solid #de0305 ;text-align: center;line-height: 30px;}

.c-left .b-ui1 table td{border-bottom: 1px solid #F1F1F1 ;text-align: center;line-height: 30px;}

.c-right{position: relative;width: 300px;float: right;border: 1px solid #FF0000;border-top-left-radius: 5px;border-top-right-radius:5px;}
.c-right .b-ui4{position: relative;width: 300px;background: #FF0000;}
.c-right .b-ui4 h2{padding-left:5px; color: #fff;}

.c-right .b-ui2{width: 300px;height: 970px;}
