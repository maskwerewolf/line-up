@charset "utf-8";
html{color:#666;background:#fff}

body,div,input,h1,h2{padding: 0px;margin: 0px;}
body,button,input,select,textarea{font:12px/1.5 tahoma,arial,\5b8b\4f53}

table{border-collapse:collapse;border-spacing:0}


.pr-10{padding-right:10px}
.num{font-family: Georgia, "Times New Roman", Times, serif; font-size: 20px;color: #666;font-weight: bold;}
.rmb{font-family: Georgia, "Times New Roman", Times, serif; font-size: 20px;color: #53a000;font-weight: bold;}
.color1{color: #3d8bec}
.error{font-size: 14px;color: #de0305}
.header{position: relative;margin: 0;background: #de0305 url(<%= asset_path 'h-top.png' %> ) repeat-x;}

.header .header-top{width: 950px;margin: auto;position: relative}
.header .header-top .bg1{background: url(<%= asset_path 'h-body.png'%>)no-repeat;height: 500px;padding-top: -300px;}

.header .header-bottom{background: url(<%= asset_path 'h-bottom.png' %>) repeat-x;position: relative;height: 233px;margin-top: -200px;}

.container{position: relative;margin: 0 auto;width: 950px;height: auto;}

.h-ui1{position: relative;margin-top: 10px;border: 1px solid  #DE0305}
.h-ui1 h2{background:#DE0305;}
.h-ui1 h2{display:block;overflow:hidden;position:relative;color:#fff;clear:both;}
.h-ui2{position:relative;border-top:1px solid #DE0305;}
.h-ui2 table{margin-top: 5px;}

.h-ui3{position:relative;margin-left: 200px;margin-bottom: 20px;}
.h-ui3 input,.h-ui3 img,h-ui3 button{vertical-align:middle;}
.h-ui3 .inp1{border: 1px solid #DE0305;;width: 200px;height: 30px;padding-left:15px;}
.h-ui3 .inp2{border: 1px solid #DE0305;;width: 50px;height: 30px;margin-left:10px;}
.h-ui3 span{margin-left: 10px;}
.h-ui3 .inp3{border: 1px solid #DE0305;;width: 50px;height: 30px;margin-left:5px;padding-left: 15px; }
.h-ui3 .btn1{background: #DE0305;height: 32px;border:0;width: 150px;text-align:center;color: #fff}

.c-left{position: relative;width: 640px; float: left;}
.c-left .b-ui3{position: relative;margin-top: 10px;width: 640px;}
/*.c-left .b-ui3 h2{color: #DE0305}*/
.c-left .b-ui1{position: relative ;margin-top: 10px;width: 640px; height: 700px; border: 1px solid #DE0305;}


.c-right{position: relative;width: 300px;float: right;}
.c-right .b-ui4{position: relative;margin-top: 10px;width: 300px;}
.c-right .b-ui2{width: 300px;height: 870px; border: none;border-top:2px solid #666;background: #fff7e4}

.c-left .b-ui3 h2{color: #DE0305}
.c-right .b-ui4 h2{color: #333}


.b-ui3 .tab ,.b-ui4 .tab{border: 1px solid #f1f1f1;border-top:2px solid #DE0305;}
.b-ui3 .tab p,.b-ui4 p{line-height: 20px;padding: 0;margin: 5px;}




.scroll ul,.scroll li{list-style:none; margin:0; padding:0;}
.scroll li{padding-left: 10px; line-height:25px; overflow:hidden;}


.b-ui1 table th{border-bottom: 1px solid #DE0305 ;text-align: center;line-height: 30px;background: #DE0305;color: #fff }
.b-ui1 table td{border-bottom: 1px solid #F1F1F1 ;text-align: center;line-height: 30px;}
.footer{position: relative;width: auto;height: 100px;background: #fff;margin-top: 20px;border-top: 1px solid #ddd;}
.footer .content{position: relative;margin: 0 auto;width: 950px;height: auto;}

.pagination{font-size:14px;font-family: Microsoft YaHei;}
.pagination a{text-decoration: none;color:#DE0305;padding-left: 5px;}
.pagination .current{color: #3d8bec;padding-left: 5px;}
.pagination .previous_page,.pagination .next_page{text-decoration: underline}
.pagination a:hover{text-decoration: underline;color: #DE0305}
.pagination .disabled{display: none}



/*system*/
.shell{width: 950px;margin: auto}
.btn-ok{border:1px solid #DE0305;background: #DE0305;color: #fff;width: 50px;}
.input-amount{border: 1px solid #DE0305;width: 80px;font-size:18px;color: #53a000 }
.s-header{background: #DE0305;height: 90px;border-bottom: 1px solid #DE0305}
.s-header .top{height: 50px;}
.s-header .top h1{float:left;padding-top: 12px;color: #fff}
.s-header .top .top-navigation{float: right;padding-top: 12px;color: #fff}
.s-header .top .top-navigation a{color:#fff;text-decoration: underline;margin-left: 2px;}
.s-header .top .top-navigation a:hover{color:#fff;text-decoration: none;}
.s-header .top .top-navigation span{color:#fff;}

.s-header .navigation{height: 40px;margin-top: 0px;line-height: 30px;}
.s-header .navigation a {text-decoration: none;color: #fff;padding: 0 15 0 0;font-weight: bold;font-size: 12px;}
.s-header .navigation ul{list-style: none;padding: 0;margin: 0}
.s-header .navigation ul li{float: left;margin-right:5px;}


.s-container .main{margin-top: 20px;border: 1px solid #DE0305;}
.s-container .main table{text-align: center;}
/*.s-container .main table tr{border-bottom: 1px solid #F1F1F1}*/
.s-container .main table th{line-height: 30px;background: #DE0305;color: #fff}
.s-container .main table td{line-height: 25px;border-bottom: 1px solid #F1F1F1}
.s-footer{height: 30px;background:#fff;margin-top: 10px;border-top:1px solid #f1f1f1}
