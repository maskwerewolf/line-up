
<div class="s-header">
    <div class="shell">
        <div class="top">
            <h1>line up</h1>
            <div class="top-navigation">
                当前登录<a href="">苍井空</a>
                <span>|</span><a href="">退出</a>
            </div>
        </div>

        <div class="navigation">
            <ul>
                <li><a href="#">TODO</a></li>
                <li><a href="#">参数设置</a></li>
                <li><a href="#">数据统计</a></li>
            </ul>
        </div>
    </div>

</div>

<div class="s-container">
    <div class="shell">
        <div class="main">
            <table width="100%">
                <colgroup>
                    <col width="15"/>
                    <col width="25"/>
                    <col width="10"/>
                    <col width="15"/>
                    <col width="15"/>
                    <col width="20"/>
                </colgroup>
                <thead>
                <tr>
                    <th>队列号</th>
                    <th>用户账号</th>
                    <th>次数</th>
                    <th>上次金额</th>
                    <th>本次金额</th>
                    <th>操作</th>
                </tr>
                </thead>
                <tbody>

            <%@acquisition_queue.each do |q|%>
                <tr>
                    <td><span class="num"><%=q.idle_queue_num%></span></td>
                    <td><%=q.account_name%></td>
                    <td><strong><%=q.acquisition_amount_count%></strong></td>
                    <td><span class="rmb"><%=q.acquisition_amount_total%></span></td>
                    <td><input type="text" class="input-amount rmb" value="<%=UserQueue::DEFAULT_AMOUNT/1000%>"/></td>
                    <td><button class="btn-ok">确定</button></td>
                </tr>
             <% end %>
                </tbody>
            </table>
        </div>
    </div>
</div>
<div class="s-footer">
    <div class="shell">
        <h2>FUCK</h2>
    </div>
</div>
